    "version": "0.2.0",
    "configurations": [
        {
            "name": "Data processing SimCLR",
            "type": "python",
            "request": "launch",
            "program": "data_processing/SimCLR.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Data processing Mixing Up",
            "type": "python",
            "request": "launch",
            "program": "data_processing/Mixing-up.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "SleepECG Pre Train",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/code/TFC",
            "env": {
                "PYTHONPATH": "${cwd}"
            },
            "args": [
                "--training_mode",
                "pre_train"
            ],
            "justMyCode": true
        },
        {
            "name": "SleepECG2 -> Epilepsy Fine Tune",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/code/TFC",
            "env": {
                "PYTHONPATH": "${cwd}"
            },
            "args": [
                "--training_mode",
                "fine_tune_test"
            ],
            "justMyCode": true
        }
    ]
}